<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Get CRUD</title>
    <link rel="stylesheet" href="/css/style.css" />
</head>

<body>
    <div class="Table-container">

        <div class="title">List User</div>
        <table>
            <tr>
                <th>Email</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Address</th>
                <th>Actions</th>
            </tr>
            <% listUsers.forEach(function(user) { %>
                <tr>
                    <td>
                        <%=user.email %>
                    </td>
                    <td>
                        <%=user.firstName %>
                    </td>
                    <td>
                        <%=user.lastName %>
                    </td>
                    <td>
                        <%=user.address %>
                    </td>
                    <td>
                        <a href="/edit-crud/<%= user.id %>" type="button">
                            Edit
                        </a>
                        <a href="/delete-crud/<%= user.id %>" type="button">
                            Delete
                        </a>
                    </td>
                </tr>
                <% }); %>
        </table>
    </div>
</body>

</html>